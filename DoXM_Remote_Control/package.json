{
  "name": "doxm_remote_control",
  "productName": "DoXM Remote Control",
  "version": "2019.02.01",
  "description": "The remote control client for DoXM.",
  "main": "Main.js",
  "scripts": {
    "start": "tsc && electron .",
    "rebuild": "npm rebuild --runtime=electron --target=14.2.0 --disturl=https://atom.io/download/atom-shell --abi=97"
  },
  "author": {
    "name": "Translucency Software",
    "email": "translucency_software@outlook.com",
    "url": "https://lucency.co"
  },
  "homepage": "https://doxm.app",
  "repository": {
    "type": "git",
    "url": "git://github.com/lucent-sea/doxm.git"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "electron": "^14.2.0",
    "electron-builder": "^22.13.1",
    "npm-check-updates": "^11.8.5"
  },
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "@microsoft/signalr": "^5.0.11",
    "robotjs": "^0.6.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "build": {
    "appId": "DoXM Remote Control",
    "win": {
      "icon": "build/icon.ico",
      "target": "portable",
      "extraFiles": "Resources/DoXM_Switch_Watch.exe"
    },
    "nsis": {
      "installerIcon": "build/icon.ico"
    }
  }
